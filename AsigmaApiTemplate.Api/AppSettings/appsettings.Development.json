{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/.log",
          "rollingInterval": "Hour",
          "outputTemplate": "[{Environment} @clientId:{ClientId} {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} @correlationId:{CorrelationId} @userId:{UserId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost; User Id=#; Database=#; Port=#; Password=#; SSL Mode=Prefer; Trust Server Certificate=true"
  },
  
  "ServiceBaseUrls": {
    "Weather": "https://test.weather.com"
  },
  "Identity": {
    "AuthAddress": "#",
    "ClientId": "#",
    "ClientSecret": "#",
    "GrantType": "#",
    "Scope": "#"
  }
}