{
  "Service": "#",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/.log",
          "rollingInterval": "Hour",
          "outputTemplate": "[{Environment} @clientId:{ClientId} {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} @correlationId:{CorrelationId} @userId:{UserId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ServiceBaseUrls": {
    "Weather": "<Put BaseUrl for an external service you wish to communicate with. It should end with a '/'>"
  },
  "Identity": {
    "AuthAddress": "<Url for the authentication authority>",
    "ClientId": "<>",
    "ClientSecret": "<>",
    "GrantType": "<>",
    "Scope": "<>"
  }
}