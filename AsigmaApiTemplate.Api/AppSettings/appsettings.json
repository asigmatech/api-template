{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "AllowedHosts": "*",
    "Serilog": {
      "Using": [
        "Serilog.Sinks.File",
        "Serilog.Sinks.Seq"
      ],
      "MinimumLevel": {
        "Default": "Information"
      },
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "logs/.log",
            "rollingInterval": "Hour",
            "outputTemplate": "[{Environment} @clientId:{ClientId} {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} @correlationId:{CorrelationId} @userId:{UserId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
          }
        },
        {
          "Name": "Seq",
          "Args": {
            "apiKey": "#",
            "serverUrl": "http://localhost:7249",
            "rollingInterval": "Hour",
            "outputTemplate": "[{Environment} @clientId:{ClientId} {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} @correlationId:{CorrelationId} @userId:{UserId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
          }
        }
      ]
    }
  }

